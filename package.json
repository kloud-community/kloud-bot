{
  "name": "kloud-bot",
  "packageManager": "bun@1.0.14",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "lint": "biome lint ./src",
    "fix": "biome format ./src --write && biome lint ./src --apply",
    "ci": "biome ci ./src",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.0",
    "@types/lint-staged": "^13.2.2",
    "bun-types": "latest",
    "husky": "^9.0.0",
    "lint-staged": "^16.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "discord.js": "^14.14.1"
  }
}
